func () > main > (int) {
    Animal a = make Animal(5);

    return a.id; 
}

func (int a, int b) > add > (int) {
    return a + b;
}

tmpl Animal {
    int id;

    constructor (int id) {
        this.id = id;
    }

    func () > id > (int) {
        return this.id;
    }

    func (int i) > changeID > () {
        this.id = i;
    }
}
