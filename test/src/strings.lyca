func () > main > () {
    Person p = make Person < ("Kevin", 5);
    p.id.setId(10);

    string message = "%d"
    printf(message.toCStr(), p.getId());
}

tmpl Id {
    int id;

    func (int id) > setId > () {
        this.id = id;
    }
}

tmpl Person {
    Id id;

    constructor < (string name, int age) {
        this.id = make Id < ();
    }

    func (int id) > setId > () {
        this.id.setId(id);
    }

    func () > getId > (int) {
        return this.id.id;
    }
}
