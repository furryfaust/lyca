tmpl Calculator {
    []char name;
    func (int, int) > (int) add;

    constructor ([]char name) {
        this.name = name;

        add = func (int a, int b) > (int) {
            return a + b;
        };
    }

    func (int a, int b) > sub > (int) {
        return a - b;
    }
}

func () > main > () {
    Calculator calc = make Calculator();

    int sum = calc.add(1, 5);
}
